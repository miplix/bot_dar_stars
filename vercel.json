{
  "version": 2,
  "builds": [
    {
      "src": "api/webhook.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/setup_webhook.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/webhook",
      "dest": "api/webhook.py"
    },
    {
      "src": "/api/setup-webhook",
      "dest": "api/setup_webhook.py"
    }
  ],
  "env": {
    "PYTHONPATH": "."
  }
}

