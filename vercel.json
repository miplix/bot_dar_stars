{
  "version": 2,
  "rewrites": [
    {
      "source": "/api/webhook",
      "destination": "/api/webhook.py"
    },
    {
      "source": "/api/setup-webhook",
      "destination": "/api/setup_webhook.py"
    }
  ],
  "env": {
    "PYTHONPATH": "."
  },
  "functions": {
    "api/*.py": {
      "runtime": "@vercel/python@3.11.9"
    }
  }
}

